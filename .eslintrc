{
    "extends": ["react-app", "react-app/jest", "prettier"],
    "plugins": ["prettier"],
  
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "printWidth": 150,
          "endOfLine": "auto"
